@model IEnumerable<UserViewModel>
@inject UserService userservise
@{
    ViewData["Titel"] = "Admin - Dashboard";
    var users = await userservise.GetAllUsersAsync();
}



   <div class="container col-4 justify-content-center">
      @foreach (var user in users)
      {
        <div class="col-md-4">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">@user.FirstName @user.LastName</h5>
              <p class="card-text">@user.UserRoles</p>
            </div>
          </div>
        </div>
      }
    </div>






